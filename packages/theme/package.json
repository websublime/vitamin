{
  "name": "@websublime/vitamin-theme",
  "version": "0.0.0",
  "description": "Base theme for Vitamin",
  "scripts": {
    "debug": "NODE_ENV=local NODE_OPTIONS='--enable-source-maps --inspect --loader tsx' node src/any.ts",
    "dev": "pnpm build && node development.js --watch --serve",
    "build": "rm -rf ./dist && pnpm parse && pnpm dts && pnpm types && pnpm clean",
    "parse": "NODE_ENV=production node build.js",
    "dts": "tsc --declaration --emitDeclarationOnly --project tsconfig.declaration.json",
    "types": "mkdir dist/docs && api-extractor run --local",
    "clean": "rm -rf ./temp && rm -rf ./tsconfig.tsbuildinfo && rm -rf tsconfig.declaration.tsbuildinfo",
    "lint": "eslint . --ext .js,.ts,.mjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@akrc/esbuild-plugin-clean": "^0.1.3",
    "@deanc/esbuild-plugin-postcss": "1.0.2",
    "@microsoft/api-documenter": "^7.22.21",
    "@microsoft/api-extractor": "^7.36.0",
    "autoprefixer": "10.4.14",
    "color-convert": "2.0.1",
    "esbuild": "^0.18.6",
    "glob": "^10.3.0",
    "postcss": "8.4.24",
    "tailwindcss": "3.3.2"
  },
  "keywords": [
    "theme",
    "websublime",
    "css",
    "ui"
  ],
  "author": {
    "email": "hello@miguelramos.dev",
    "name": "miguelramos",
    "url": "https://github.com/miguelramos"
  },
  "license": "ISC",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/@types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/@types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./version.js": {
      "types": "./version.d.ts",
      "default": "./version.js"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:websublime/vitamin.git",
    "directory": "packages/theme"
  },
  "files": [
    "dist/",
    "./LICENSE.md",
    "./README.md"
  ]
}

{
  "name": "@websublime/vitamin-core",
  "version": "0.1.2",
  "description": "Core lit functions",
  "type": "module",
  "scripts": {
    "debug": "NODE_ENV=local NODE_OPTIONS='--enable-source-maps --inspect --loader tsx' node src/any.ts",
    "dev": "NODE_ENV=development node development.mjs --watch --serve",
    "build": "rm -rf ./dist && pnpm parse && pnpm dts && pnpm types && pnpm clean",
    "parse": "NODE_ENV=production node build.mjs",
    "dts": "tsc --declaration --emitDeclarationOnly --project tsconfig.declaration.json",
    "types": "mkdir dist/docs && api-extractor run --local",
    "clean": "rm -rf ./temp && rm -rf ./tsconfig.tsbuildinfo && rm -rf tsconfig.declaration.tsbuildinfo",
    "lint": "eslint . --ext .js,.ts,.mjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@akrc/esbuild-plugin-clean": "^0.1.3",
    "@microsoft/api-documenter": "^7.22.21",
    "@microsoft/api-extractor": "^7.36.0",
    "esbuild": "^0.18.6",
    "esbuild-plugin-lit-css": "^2.0.4",
    "glob": "^10.3.0"
  },
  "dependencies": {
    "lit": "3.0.0-pre.0"
  },
  "keywords": [
    "core",
    "websublime",
    "lit",
    "ui"
  ],
  "author": {
    "email": "hello@miguelramos.dev",
    "name": "miguelramos",
    "url": "https://github.com/miguelramos"
  },
  "license": "ISC",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/@types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./version.js": {
      "types": "./dist/version.d.ts",
      "default": "./dist/version.js"
    },
    "./decorators.js": {
      "types": "./dist/utilities/decorators.d.ts",
      "default": "./dist/utilities/decorators.js"
    },
    "./directives.js": {
      "types": "./dist/utilities/directives.d.ts",
      "default": "./dist/utilities/directives.js"
    },
    "./event-controller.js": {
      "types": "./dist/utilities/event-controller.d.ts",
      "default": "./dist/utilities/event-controller.js"
    },
    "./helpers.js": {
      "types": "./dist/utilities/helpers.d.ts",
      "default": "./dist/utilities/helpers.js"
    },
    "./html.js": {
      "types": "./dist/utilities/html.d.ts",
      "default": "./dist/utilities/html.js"
    },
    "./web-component.js": {
      "types": "./dist/utilities/web-component.d.ts",
      "default": "./dist/utilities/web-component.js"
    },
    "./inspect-controller.js": {
      "types": "./dist/utilities/inspect-controller.d.ts",
      "default": "./dist/utilities/inspect-controller.js"
    },
    "./storage.js": {
      "types": "./dist/utilities/storage.d.ts",
      "default": "./dist/utilities/storage.js"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:websublime/vitamin.git",
    "directory": "packages/core"
  },
  "files": [
    "dist/",
    "./LICENSE.md",
    "./README.md"
  ]
}

    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-new-with-error": "^2.0.0",

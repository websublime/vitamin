  "scripts": {
    "build": "yarn clean && yarn build:rollup && yarn build:css && yarn theme:copy",
    "build:css": "postcss ./src/theme/index.css -o ./dist/theme.css",
    "build:css:watch": "postcss ./src/theme/index.css -o ./dist/theme.css -w",
    "build:rollup": "rollup -c && rollup -c --environment MINIFY",
    "build:watch": "concurrently \"rollup -c --watch\" \"yarn build:css:watch\"",
    "clean": "rm -rf ./dist",
    "lint": "eslint . --ext .vue,.js,.ts",
    "test": "jest",
    "theme:copy": "cp -R ./src/theme ./dist"
